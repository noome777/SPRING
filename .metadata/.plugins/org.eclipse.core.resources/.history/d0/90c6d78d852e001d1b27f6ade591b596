package com.kh.app01;

public class HomeService {

	private HomeDao dao;
	
	//나를 태어나게 하려면 dao를 가져다 줘라 !!
	public HomeService(HomeDao dao) {
		System.out.println("홈 서비스 객체 태어남");
		this.dao = dao;
	}
	
	public int home() {
		System.out.println("service 호출됨");
		int result = dao.home();
		return result;
	}
	
	

}
